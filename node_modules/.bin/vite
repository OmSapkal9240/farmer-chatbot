#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules/vite/bin/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules/vite/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules/vite/bin/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules/vite/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/vite@7.3.0_@types+node@25.0.3_jiti@1.21.7/node_modules:/mnt/c/Users/om852/OneDrive/Desktop/PICT HACK/farmer-chatbot/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
